{
    "name": "ramsey/tools-test",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ramsey\\ToolsTest\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ben Ramsey",
            "email": "ben@benramsey.com"
        }
    ],
    "require": {
        "amphp/amp": "^3.0",
        "revolt/event-loop": "^1.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": false,
            "forward-command": true,
            "target-directory": "vendor-bin"
        }
    },
    "scripts": {
    }
}
